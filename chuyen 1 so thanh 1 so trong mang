#include<iostream>
#include<math.h>
using namespace std;
int main(){
	int a[100] , n ;
	cout << " nhap n=" ;
	cin >> n ;
	for (int i =0 ; i <n ; i++ ){
		cout << "a[" << i << "]=";
		cin >> a[i];
		}
	int x , y ;
	cout << " x= " ; cin >> x;
	cout << " y= " ; cin >> y;
	for(int i = 0 ; i< n ; i++){
		if(a[i]==x)
		a[i]=y;
		cout << a[i] << " ";
	}	
	
	
return 0 ;
}
