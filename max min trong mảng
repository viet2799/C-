#include<iostream>
using namespace std;
int main()
{
    int n,a[50],i,max=a[0];
	cout<<"n= ";
	cin>>n;
	for(i=0;i<n;i++)
	{
		cout<<"a["<<i<<"]= ";
		cin>>a[i];
	}
	for(i=0;i<n;i++)
	{
		if(a[i]>max)
		max=a[i];
	}
	cout<<"Max cua day = "<<max<<endl;
	int min=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]<min)
		min=a[i];
	}
	cout<<"Min cua day = "<<min;
}
